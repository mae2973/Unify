<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/placeholder">
    <fragment android:id="@+id/placeholder" >
        <action
            android:id="@+id/action_placeholder_to_participants"
            app:destination="@id/participants" />
    </fragment>
    <activity
        android:id="@+id/participants"
        android:name="com.example.unify.Participants"
        android:label="activity_participants"
        tools:layout="@layout/activity_participants" />
</navigation>