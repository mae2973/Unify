<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".C_InterfacePrincipale"
    tools:ignore="ExtraText">


    <include
        android:id="@+id/include"
        layout="@layout/content_main"
        app:layout_anchor="@+id/include"
        app:layout_anchorGravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="NotSibling" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="154dp"
        android:layout_height="71dp"
        android:layout_marginStart="133dp"
        android:layout_marginTop="84dp"
        android:text="Musique"
        android:textColor="@color/white"
        android:textSize="37dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="77dp"
        android:layout_height="74dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/include"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/include"
        app:srcCompat="@drawable/logo_1" />


    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="138dp"
        android:layout_height="138dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="201dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icone_musique" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="65dp"
        android:layout_height="28dp"
        android:layout_marginTop="204dp"
        android:layout_marginEnd="78dp"
        android:text="Titre"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="65dp"
        android:layout_height="22dp"
        android:layout_marginTop="232dp"
        android:layout_marginEnd="77dp"
        android:text="Auteur"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="323dp"
        android:layout_height="15dp"
        android:layout_marginTop="392dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/barre_musique" />


    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="362dp"
        android:text="0:00"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="362dp"
        android:layout_marginEnd="9dp"
        android:text="00:00"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageButton
        android:id="@+id/parametrehost2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/parametres"
        android:contentDescription="Paramètres"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/plus2"
        android:layout_width="110dp"
        android:layout_height="80dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="18dp"
        android:background="@drawable/bouton_ajout"
        android:contentDescription="Plus d'actions"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/versParticipant"
        android:layout_width="25dp"
        android:layout_height="38dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="94dp"
        android:background="@drawable/fleche_droit"
        android:contentDescription="voir participants"
        android:padding="40dp"
        android:rotationY="180"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/versSuggestion"
        android:layout_width="25dp"
        android:layout_height="38dp"
        android:layout_marginTop="94dp"
        android:layout_marginEnd="70dp"
        android:background="@drawable/fleche_droit"
        android:contentDescription="voir suggestions"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/playHost"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="270dp"
        android:layout_marginEnd="78dp"
        android:background="@drawable/play"
        android:contentDescription="Play"
        android:rotation="90"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/MusiqueSuivante"
        android:layout_width="25dp"
        android:layout_height="38dp"
        android:layout_marginTop="276dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/fleche_droit"
        android:contentDescription="Musique suivante"
        android:padding="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/MusiquePrecedente"
        android:layout_width="25dp"
        android:layout_height="38dp"
        android:layout_marginStart="228dp"
        android:layout_marginTop="276dp"
        android:background="@drawable/fleche_droit"
        android:contentDescription="Musique précédente"
        android:padding="40dp"
        android:rotationY="180"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvMusique"
        android:layout_width="374dp"
        android:layout_height="183dp"
        android:layout_marginBottom="102dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>